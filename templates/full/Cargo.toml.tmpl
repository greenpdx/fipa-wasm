[package]
name = "{{agent_name}}"
version = "0.1.0"
edition = "2021"
description = "FIPA WASM Agent - {{agent_name}}"

[lib]
crate-type = ["cdylib"]

[dependencies]
wit-bindgen = "0.36"

# Optional: for content serialization
# serde = { version = "1.0", features = ["derive"] }
